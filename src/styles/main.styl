    line-height 75px
    text-align center
    color white
    background-color #00006c
    border-radius 25px
    cursor pointer

#sp
    float left

#sp1
    margin 69px 20px 0 0

#mp1
    margin 50px 20px 0 0

#homeHangman, #hangman
